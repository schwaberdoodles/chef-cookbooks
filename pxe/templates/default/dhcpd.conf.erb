ddns-update-style none;
default-lease-time 600;
option domain-name-servers <%= @node[:pxe][:dhcp][:dns_server] %>;
max-lease-time 7200;
allow booting;
allow bootp;
class "pxeclients" {
  match if substring(option vendor-class-identifier, 0, 9) = "PXEClient";
  next-server <%= @node[:pxe][:dhcp][:pxe_server] %>;
  filename = "pxelinux.0";

}

subnet <%= @node[:pxe][:dhcp][:subnet] %> netmask <%= @node[:pxe][:dhcp][:subnet_mask] %> {
  pool {
    allow members of "pxeclients";
    range <%= @node[:pxe][:dhcp][:host_range] %>;
  }
}

